package com.edubridge.maven_project;

public class Calc2 {
	public int findSum(int x , int y) {
		return x+y;
	}
	
	public int findSub(int x , int y) {
		return x-y;
	}
	public int findMul(int x, int y) {
		return x*y;
	}
	public int findDiv(int x, int y) {
		return x/x;
	
	}

	public static void main(String[] args) {
		
		
	}

}
